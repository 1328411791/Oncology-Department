			<LocID>EXM_SLP_CCBT</LocID>                                      
			<Text>儿茶酚胺血液采样</Text>  
			</GameDBLocalizedString>
			
			<GameDBLocalizedString>      
			<LocID>EXM_SLP_CCBT_DESC</LocID>                                      
